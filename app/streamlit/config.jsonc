                    "token_limit": 100000,
                    "max_response_token": 10000,
                    "vision": false
                },
                "o3-mini": {
                    "name": "o3-mini",
                    "token_limit": 100000,
                    "max_response_token": 10000,
                    "vision": false
                }
            }
        }
